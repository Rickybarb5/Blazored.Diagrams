<MudFab Color="Color.Secondary" StartIcon="@Icons.Material.Filled.Save" OnClick="Save" Style="position:fixed; bottom:50px;right:50px"/>

@code {
    [CascadingParameter] public IDiagramService Service { get; set; }
    [Inject] IDialogService DialogService { get; set; }
    [Inject] IDiagramSerializationService SerializationService { get; set; }

    private void Save(MouseEventArgs obj)
    {
        var result = SerializationService.ToJson<Diagram>(Service.Diagram as Diagram);
        DialogService.ShowMessageBox("JSON Diagram", result);
    }

}