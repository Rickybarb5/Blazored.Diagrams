<MudFab Color="Color.Secondary" StartIcon="@Icons.Material.Filled.Upload" OnClick="Load" Style="position:fixed; bottom:50px;right:150px"/>

@code {

    [CascadingParameter] public IDiagramService DiagramService { get; set; }

    [Inject] IDialogService DialogService { get; set; }

    [Parameter] public EventCallback<Diagram> OnDiagramLoaded { get; set; }

    private async Task Load(MouseEventArgs obj)
    {
        var dialogParameters = new DialogParameters
        {
            [nameof(LoadDiagramDialog.Service)] = DiagramService
        };
        var options = new DialogOptions { CloseOnEscapeKey = true };
        var dialogReference = DialogService.ShowAsync<LoadDiagramDialog>("Dialog Keyboard Accessibility Demo", dialogParameters, options).Result;
        var dialogResult = await dialogReference.Result;
        if (dialogResult.Canceled)
        {
            StateHasChanged();
        }
        else
        {
            await OnDiagramLoaded.InvokeAsync((Diagram)dialogResult.Data);
            StateHasChanged();
        }
    }

}